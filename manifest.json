{
	"name": "Event Tracking Tracker",
	"version": "1.0.3",
	"manifest_version": 2,
	"description": "Plugin to track GA Event Tracker event calls that persists across pages",
	"browser_action": {
	    "default_icon": "ett.png",
	    "popup": "popup.html"
  	},
  	"icons": {
  		"16": "ett16.png",
  		"48": "ett48.png",
  		"128": "ett.png"
  	},
	"background": {
		"scripts": ["background.html"],
		"persistent": true
	},
	"permissions": [
		"webRequest",
        "tabs", 
        "http://*/*", 
        "https://*/*"
    ]
}