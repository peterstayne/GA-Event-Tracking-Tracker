{
	"name": "Event Tracking Tracker",
	"version": "1.1.0",
	"manifest_version": 3,
	"description": "Plugin to track GA Event Tracker event calls that persists across pages",
	"action": {
	    "default_icon": "ett.png",
	    "default_popup": "popup.html"
  	},
  	"icons": {
  		"16": "ett16.png",
  		"48": "ett48.png",
  		"128": "ett.png"
  	},
	"background": {
		"service.worker": ["background.js"]
	},
	"host_permissions": [
        "http://*/*", 
        "https://*/*"
	],
	"permissions": [
		"webRequest",
		"storage",
        "tabs"
    ]
}