{
	"name": "Event Tracking Tracker",
	"version": "1.1.5",
	"manifest_version": 3,
    "description": "Plugin to track Google Analytics Event Tracker event calls",
	"action": {
	    "default_icon": "ett.png"
  	},
  	"icons": {
  		"16": "ett16.png",
  		"48": "ett48.png",
  		"128": "ett.png"
  	},
	"background": {
		"service_worker": "background.js"
	},
	"host_permissions": [
        "http://*/*", 
        "https://*/*"
	],
	"permissions": [
		"webRequest",
		"storage",
		"tabs"
    ]
}